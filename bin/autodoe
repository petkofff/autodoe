#!/usr/bin/env node

var program = require('commander');

program
	.version('0.0.0')
	.usage('[command]')
	.command('init', 'Initializes new .autodoe.sh in the working directory')
	.command('scr', 'Prints the .autodoe.sh script in the working directory')
	.command('run', 'Triggers monitoring the working directory.')
	.parse(process.argv);
